{
  "manifest_version": 3,
  "name": "PhishNet Protection",
  "version": "1.0.1",
  "description": "Advanced phishing protection at your fingertips",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "http://localhost:5000/*",
    "http://localhost:3000/*",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "PhishNet Protection",
    "default_icon": "icon.png"
  }
}
